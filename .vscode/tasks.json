{
    "tasks": [
        {
            "label": "accurate cmake build",
            "type": "shell",
            "command": "cmake --preset accurate;cmake --build build/acrt",
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "debug cmake build",
            "type": "shell",
            "command": "cmake --preset debug;cmake --build build/dbg",
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "release cmake build",
            "type": "shell",
            "command": "cmake --preset release;cmake --build build/rls",
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ],
    "version": "2.0.0"
}